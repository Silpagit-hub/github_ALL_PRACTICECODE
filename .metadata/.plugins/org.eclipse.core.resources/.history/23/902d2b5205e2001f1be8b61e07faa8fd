package com.javaCore;

public class PrimeNumber {

	public static void main(String[] args) {

		int number=5;
		for(int i=2;i<=number/2;i++) {
			if(number%i==0) {
				System.out.println(number+" is not prime number");
			}
			else {
				System.out.println(number+" is a prime number");
			}
		}
	}

}
